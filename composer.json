{
    "name": "fidanf/slim-flex",
    "description": "slim-flex",
    "keywords": ["framework", "slim"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Fidanza Frank",
            "email": "fidanza.frank@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "slim/slim": "^3.12",
        "vlucas/valitron": "^1.4",
        "vlucas/phpdotenv": "^3.4",
        "league/container": "^2.4",
        "hassankhan/config": "^2.0",
        "twig/twig": "^2.11 ",
        "illuminate/database": "^5.8",
        "illuminate/pagination": "^5.8",
        "robmorgan/phinx": "^0.10.8",
        "swiftmailer/swiftmailer": "^6.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
          "App\\": "app"
        },
        "files": ["app/helpers.php"]
    },
    "scripts": {
        "post-install-cmd": [
            "@copy-env"
        ],
        "copy-env": "@php -r \"file_exists('.env') ?: copy('.env.dist', '.env');\""
    }
}
